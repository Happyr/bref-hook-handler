{
    "name": "happyr/bref-hook-handler",
    "type": "library",
    "description": "Simple way to handle Bref Lambda hooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "async-aws/code-deploy": "^1.0",
        "async-aws/lambda": "^1.0",
        "bref/bref": "^0.5.20 || ^1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Happyr\\BrefHookHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Happyr\\BrefHookHandler\\": "tests/"
        }
    }
}
